{
  "schemaVersion": 1,
  "id": "arthurscandles",
  "version": "${version}",
  "name": "Arthur's Candles",
  "description": "A minecraft mod that adds Candles that do stuff",
  "authors": [],
  "contact": {},
  "license": "GPL-3.0",
  "icon": "assets/arthurscandles/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "com.asteristired.arthurscandles.client.ArthursCandlesDataGenerator"
    ],
    "client": [
      "com.asteristired.arthurscandles.client.ArthursCandlesClient"
    ],
    "main": [
      "com.asteristired.arthurscandles.ArthursCandles"
    ]
  },
  "mixins": [
    "arthurscandles.mixins.json",
    {
      "config": "arthurscandles.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
